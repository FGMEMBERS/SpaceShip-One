<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>interior2.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>intsso1</object-name>
    <object-name>intsso2</object-name>

    <!-- Objets transparents -->
    <object-name>intsso3</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>intsso3</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>intsso3</object-name>
  </animation>

  <!-- Pilote -->
  <model>
    <name>Pilote</name>
    <path>Aircraft/SpaceShip-One/Models/Pilot/pilot.xml</path>
    <offsets>
      <x-m> -3.594 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -1.057 </z-m>
    </offsets>
  </model>

</PropertyList>

